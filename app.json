{
  "expo": {
    "name": "Como Pago",
    "slug": "dane_billetera_JS",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/favicon_white.png",
      "bundleIdentifier": "com.dane.comopago",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take your profile picture.",
        "NSPhotoLibraryUsageDescription": "You need to provide photo album access to choose a profile picture."
      }
    },
    "android": {
      "package": "com.dane",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "description": "",
    "extra": {
      "eas": {
        "projectId": "4e021457-ab53-47ce-bb5f-93c608217420"
      }
    }
  }
}
